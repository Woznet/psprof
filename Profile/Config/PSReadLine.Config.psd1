@{
    EditMode = 'Windows'
    HistoryNoDuplicates = $true
    HistorySearchCursorMovesToEnd = $true
    Colors = @{
        "Command" = [ConsoleColor]::Magenta
        "Parameter" = [ConsoleColor]::Yellow
        "String" = [ConsoleColor]::Green
        "Number" = [ConsoleColor]::Magenta
        "Type" = [ConsoleColor]::White
        "Operator" = [ConsoleColor]::DarkGray
        "Variable" = [ConsoleColor]::DarkCyan
        "Keyword" = [ConsoleColor]::Red
        "Comment" = [ConsoleColor]::DarkGreen
        "Selection" = [ConsoleColor]::DarkGray
        "Emphasis" = [ConsoleColor]::White
        "Error" = [ConsoleColor]::DarkRed
    }
}
